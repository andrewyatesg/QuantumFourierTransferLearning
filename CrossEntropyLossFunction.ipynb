    "import math \n",
    "def CrossEntropy_loss(labels,predictions):\n",
    "    loss=0\n",
    "    for l,p in zip(labels,predictions):\n",
    "        loss=-l*math.log(p)-(1-l)*math.log(p)\n",
    "    loss=loss/len(labels)\n",
    "    return loss "
   ]
